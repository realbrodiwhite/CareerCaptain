# The run command to start your Next.js application
run = "npm run dev"

# The entrypoint file for your project
entrypoint = "src/app/page.tsx"

# Additional settings (optional)
language = "nodejs"
# Set the working directory to the project root
working_directory = "."

# Set the default repl shell command
shell = "bash"
modules = ["nodejs-20:v8-20230920-bd784b9"]

[[ports]]
localPort = 3000
externalPort = 80
